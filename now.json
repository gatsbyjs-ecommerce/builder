{
  "version": 2,
  "name": "",
  "build": {
    "env": {}
  },
  "builds": [
    { "src": "admin/package.json", "use": "@now/static-build" },
    { "src": "web/package.json", "use": "@now/static-build" },
    { "src": "api/**/*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "^/studio/static/(.*)", "dest": "/admin/static/$1" },
    { "src": "^/studio/(.*)", "dest": "/admin/index.html" },
    { "src": "^/api/(.*)", "dest": "/api/$1" },
    { "src": "^/(.*)", "dest": "/web/$1" }
  ],
  "github": { "autoJobCancelation": true }
}
